#include <iostream>

int main()
{
    int array [] = {-5,-7,0,12,46} ;
    int sun= 0;
    int count =0;
    //int get = sizeof(array) / sizeof(array[0]);
   // std::cout<<get;
    for(int i=0; i<5;i++){
        if (array[i]>0){
            sun = sun+ array[i];
            count++;
        }
    }
    if(count > 0){
        double average = sun/count;
        std::cout<<average;
        
    }
    else{
        std::cout<<"there is no positive element in array";
    }
   
    
    std::cout<<"\nHello World";

    return 0;
}
